-- Table: public.product_beekeeping

-- DROP TABLE IF EXISTS public.product_beekeeping;

CREATE TABLE IF NOT EXISTS public.product_beekeeping
(
    id integer NOT NULL DEFAULT nextval('products_beekeeping_id_seq'::regclass),
    name text COLLATE pg_catalog."default" NOT NULL,
    description text COLLATE pg_catalog."default" NOT NULL,
    price money NOT NULL,
    photo_url text COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT products_beekeeping_pkey PRIMARY KEY (id),
    CONSTRAINT products_beekeeping_name UNIQUE (name)
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.product_beekeeping
    OWNER to root;